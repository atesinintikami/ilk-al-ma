import random



a = {
         "NBR": "Na'ber ,nasılsın",
         "OKEY": "Tamam",
         "LOL": "Komik bir şeye verilen cevap",
         }  
         
word = input("Anlamadığınız bir kelime var mı? Varsa büyük harfle yazın): ")

         
if word in a.keys():
   print(a[word])


    
else:
   print("Bu kelimemiz sözlükte yok")  
